<div class='container-fluid' style='background-color:#ECF0F1' >
  <div class='panel panel-default'>
    <div class='panel-body'>
      <div class='row' >
        <div class='col-lg-12'>
          <ul class='list-inline' style='margin-bottom:0px;'>
            <li class='pull-left'>
              <a href='javascript:history.back()'><h6 style='font-weight:700;font-size:16px;color:#2980B9'><span class="fui-arrow-left"></span>Back</h6></a>
            </li>
            
        </div>
      </div>
    </div>
  </div>

  <div class='panel panel-default' >
      <div class='panel-body'>
        <div class='row' >
          <div class='col-xs-12'>
            <div class='row'>
              <div class='col-lg-12'>
                <ul class="list-inline" style='margin-bottom:0px'>
                  <li>
                    <h6 style='font-weight:700;font-size:16px;margin-bottom:0px'><%= @listing.name %></h6>
                  </li>
                  <li>
                    <p style='font-size:14px;margin-bottom:0px'><%= @listing.city %>, <%= @listing.state %></p>
                  </li>
              </div>
              <div class='col-lg-12' style='margin-top:0px'>
                <ul class="list-inline" style='margin-bottom:0px'>
                  <li>
                    <span class="fui-star" style='color:#2980B9'></span>
                    <span class="fui-star" style='color:#2980B9'></span>
                    <span class="fui-star" style='color:#2980B9'></span>
                    <span class="fui-star" style='color:#2980B9'></span>
                    <span class="fui-star" style='color:#2980B9'></span>
                  </li>
                  <li>
                    <p style='font-size:14px;color:#2980B9;margin-bottom:0px'>5/5</p>
                  </li>
                  <li>
                    <p style='font-size:14px;color:#2980B9;margin-bottom:0px'>3 reviews</p>
                  </li>
                </ul>
              </div>
               <div class='col-lg-12' style='margin-top:0px'>
                <% @listing.tags.each do |tag| %>
                <button class='tag' style='color:#2980B9;border:2px solid #2980B9;background-color:transparent'><%= tag %></button>
                <% end %>
              </div>
              <div class='col-lg-12'>
                <p style='font-size:14px;margin-top:5px'><%= @listing.description %></p>
              </div>
              <div class='col-lg-12' style='margin-bottom:10px'>
                <ul class="list-inline" style='margin-bottom:0px'>
                  <li>
                    <button type="submit" class="btn btn-primary" style='background-color:#2980B9'>Email</button>
                  </li>
                  <li>
                    <button type="submit" class="btn btn-primary" style='background-color:#2980B9'>Call</button>
                  </li>
                  
              </div>
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class='panel panel-default' style='margin-left:auto;margin-right:auto;border:0px;background-color:#ECF0F1;box-shadow:none;webkit-box-shadow:none'>
    <div class='panel-body'>
      <div class='row' >
        <div class='col-lg-12'>
          <h6 class='thin' style='color:#2C3E50;margin-top:8px;margin-bottom:8px;border:0px;font-size:16px'>Past Work</h6>
